  "scripts": {
    "dev": "next dev",
    "pre-build": "grep -rl 'distDir: \".next\"' next.config.js | xargs sed -i 's#distDir: \".next\"#distDir: \"build\"#g'",
    "pre-run": "grep -rl 'distDir: \"build\"' next.config.js | xargs sed -i 's#distDir: \"build\"#distDir: \".next\"#g'",
    "pre-build-mac": "grep -rl 'distDir: \".next\"' next.config.js | xargs sed -i '' 's#distDir: \".next\"#distDir: \"build\"#g'",
    "pre-run-mac": "grep -rl 'distDir: \"build\"' next.config.js | xargs sed -i '' 's#distDir: \"build\"#distDir: \".next\"#g'",
    "build": "run-script-os",
    "build:default": "npm run pre-build && next build && npm run pre-run && rm -rf .next && mv build .next",
    "build:macos": "npm run pre-build-mac && next build && npm run pre-run-mac && rm -rf .next && mv build .next",
    "start": "next start",
    "postbuild": "next-sitemap --config next-sitemap.config.js"
  },
